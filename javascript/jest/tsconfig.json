{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "rootDir": "..",
    "outDir": "dist"
  },
  "include": ["**/*.ts", "../src/**/*.ts"],
  "exclude": ["dist"]
}
